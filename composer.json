{
    "config": {
        "platform": {
            "php": "5.6"
        }
    },
    "require": {
        "php": ">=5.6",
        "ext-dom": "*",
        "johnpbloch/wordpress": ">=4.5",
        "wpackagist-plugin/classic-editor": ">=1.3",
        "wpackagist-plugin/wp-sitemap-page": ">=1.5",
        "wpackagist-plugin/wordpress-seo": ">=7.6",
        "wpackagist-plugin/contact-form-7": ">=4.4",
        "wpackagist-plugin/flamingo": ">=1.9",
        "wpackagist-plugin/inazo-flamingo-automatically-delete-old-messages": ">=1.0",
        "wpackagist-plugin/w3-total-cache": ">=0.9",
        "wpackagist-plugin/acf-code-field": ">=1.7",
        "advanced-custom-fields-pro": "dev-master",
        "post-order": "dev-master",
        "social-network": "dev-master",
        "allow-svg": "dev-master",
        "php-instagram-api/php-instagram-api": "@dev",
        "disable-comments": "dev-master",
        "scotteh/php-dom-wrapper": ">=0.7"
    },
    "extra": {
        "wordpress-install-dir": "wp",
        "installer-paths": {
            "content/mu-plugins/{$name}/": [
                "type:wordpress-muplugin",
                "wpackagist-plugin/acf-code-field"
            ],
            "content/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "content/themes/{$name}/": [
                "type:wordpress-theme"
            ]
       }
    },
    "repositories": [
        { "type": "composer", "url": "https://wpackagist.org" },
        { "type": "composer", "url": "https://jerome.gitlab.rue-de-la-vieille.fr/satis/" }
    ],
    "require-dev": {
        "sensiolabs/security-checker": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "Rdlv\\WordPress\\Theme\\": "content/themes/cardinal/class"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "php vendor/sensiolabs/security-checker/security-checker security:check"
        ]
    }
}
